<% include ../partials/header %>

    <div class='container'>

        <header class='jumbotron'>
            <div class='container'>
                <h1> camp sites</h1>
                <p> find our favorite places to camp</p>

                <p>
                    <a class='btn btn-large btn-primary' href='camps/new'> add a new camp </a>
                </p>
            </div>
        </header>


<div class='row text-center main' style='display:flex; flex-wrap: wrap;'>
    

        <% passin.forEach(function(camp){ %>
        
            <div class='col-md-3 col-sm-6'>
                <div class='thumbnail'>
                <img src="<%= camp.img %>">
                 <div class='caption '>
                <h4> <%=  camp.name %></h4>
                </div>
                                <a href="/camps/<%=camp._id%>" class='btn btn-primary'> More... </a>

                </div>
            </div>
        <% }) %>

        </div>
</div>

    <% include ../partials/footer %>
